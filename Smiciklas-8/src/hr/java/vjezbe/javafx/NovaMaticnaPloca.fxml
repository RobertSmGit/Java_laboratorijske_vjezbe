<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.java.vjezbe.javafx.MaticnaPlocaController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Naziv Proizvođača matiče ploče" />
            <Label text="Tip matiče ploče" GridPane.rowIndex="1" />
            <Label prefHeight="36.0" prefWidth="211.0" text="Maksimalan broj memorijskih modula" GridPane.rowIndex="3" />
            <Label text="Cijena" GridPane.rowIndex="4" />
            <TextField fx:id="proizvodac" GridPane.columnIndex="1" />
            <TextField fx:id="tip" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="cijena" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button mnemonicParsing="false" onAction="#spremiMaticnuPlocu" text="Spremi" GridPane.rowIndex="6" />
            <TextField fx:id="tipSucelja" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Tip sučelja za procesor" GridPane.rowIndex="2" />
            <ComboBox fx:id="brojModula" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
